<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>


<AnchorPane prefHeight="400.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.mediaview.Controller">
   <children>
      <MediaView fx:id="mediaView" fitHeight="300.0" fitWidth="500.0" />
      <Button fx:id="pauseButton" layoutX="212.0" layoutY="329.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
      <Button fx:id="playButton" layoutX="101.0" layoutY="329.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
      <Button fx:id="resetButton" layoutX="331.0" layoutY="329.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
   </children>
</AnchorPane>
